<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.geometry.Insets?>

<fx:root minHeight="0.0" minWidth="0.0" prefHeight="239.0" prefWidth="160.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
      <HBox alignment="CENTER" layoutX="6.0" layoutY="2.0" prefHeight="25.0" prefWidth="148.0" AnchorPane.leftAnchor="6.0" AnchorPane.rightAnchor="6.0">
         <children>
      				<Button fx:id="addBtn" mnemonicParsing="false" prefHeight="31.0" prefWidth="155.0" text="Add Reading" />
         </children></HBox>
		<VBox fx:id="form" alignment="TOP_CENTER" fillWidth="false" layoutY="24.0" prefHeight="218.0" prefWidth="160.0" spacing="5.0" AnchorPane.bottomAnchor="24.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="24.0">
			<children>
				<TextField fx:id="patientId" editable="false" visible="false" />
				<ChoiceBox fx:id="type" prefHeight="25.0" prefWidth="148.0" />
				<TextField fx:id="id" prefWidth="148.0" promptText="Reading ID...">
					<tooltip>
						<Tooltip text="Required" />
					</tooltip>
				</TextField>
				<StackPane prefWidth="148.0">
					<children>
						<StackPane fx:id="bloodPressStack">
							<children>
            								<HBox alignment="CENTER" prefHeight="15.0" prefWidth="170.0" spacing="5.0">
            									<children>
                  										<TextField fx:id="systolic" prefHeight="25.0" prefWidth="80.0" promptText="sys" />
                  										<TextField fx:id="diastolic" prefHeight="25.0" prefWidth="80.0" promptText="dia" />
            									</children>
            								</HBox>
							</children>
						</StackPane>
      						<TextField fx:id="value" prefWidth="148.0" promptText="Reading Value....">
      							<tooltip>
      								<Tooltip text="Required" />
      							</tooltip>
      						</TextField>
					</children>
				</StackPane>
				<DatePicker fx:id="date" prefWidth="148.0" promptText="Reading Date...">
					<tooltip>
						<Tooltip text="Required" />
					</tooltip>
				</DatePicker>
				<HBox alignment="CENTER" prefHeight="16.0" prefWidth="148.0" spacing="5.0" VBox.vgrow="NEVER">
					<children>
      						<TextField fx:id="hour" prefHeight="25.0" prefWidth="46.0" promptText="HH">
      							<tooltip>
      								<Tooltip text="0-23" />
      							</tooltip>
      						</TextField>
      						<TextField fx:id="minutes" prefHeight="25.0" prefWidth="46.0" promptText="MM">
      							<tooltip>
      								<Tooltip text="0-59" />
      							</tooltip>
      						</TextField>
      						<TextField fx:id="seconds" prefWidth="46.0" promptText="SS">
      							<tooltip>
      								<Tooltip text="0-59" />
      							</tooltip>
      						</TextField>
					</children>
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</HBox>
				<HBox>
   <children>
   					<Button fx:id="okBtn" defaultButton="true" mnemonicParsing="false" text="OK" />
   					<Button defaultButton="false" mnemonicParsing="false" onAction="#clearForm" text="Clear" />
   					<Button fx:id="cancelBtn" defaultButton="false" mnemonicParsing="false" text="Cancel" />
   </children>
				</HBox>
			</children>
			<padding>
				<Insets top="5.0" />
			</padding>
		</VBox>
	</children>
</fx:root>
